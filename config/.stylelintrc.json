{
  "extends": [
    "stylelint-config-standard",
    "stylelint-config-recess-order"
  ],
  "rules": {
    "selector-class-pattern": null,
    "selector-pseudo-class-no-unknown": [true, {"ignorePseudoClasses": ["global", "export"]}],
    "selector-pseudo-element-no-unknown": [true, {"ignorePseudoElements": ["-webkit-scrollbar", "-webkit-slider-thumb", "-moz-range-thumb"]}],
    "property-no-unknown": [true, {"ignoreProperties": ["composes", "cardBase", "pageContainer", "gridLayout"]}],
    "at-rule-no-unknown": [true, {"ignoreAtRules": ["value"]}],
    "no-duplicate-selectors": null,
    "no-descending-specificity": null,
    "block-no-empty": null,
    "keyframes-name-pattern": null,
    "declaration-property-value-allowed-list": null
  }
}
