{
  "name": "name-nosferatu",
  "version": "1.0.1",
  "description": "A React application for managing cat names and related data",
  "type": "module",
  "main": "index.js",
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guitarbeat/name-nosferatu.git"
  },
  "bugs": {
    "url": "https://github.com/guitarbeat/name-nosferatu/issues"
  },
  "keywords": [
    "react",
    "cats",
    "names",
    "management",
    "tournament",
    "voting",
    "supabase",
    "vite"
  ],
  "author": {
    "name": "Aaron",
    "email": "aaron@example.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite --config config/vite.config.ts",
    "start": "npm run dev",
    "build": "vite build --config config/vite.config.ts",
    "build:dev": "vite build --mode development --config config/vite.config.ts",
    "build:analyze": "vite build --mode analyze --config config/vite.config.ts",
    "preview": "vite preview --config config/vite.config.ts",
    "analyze": "npm run build && npx vite-bundle-analyzer build/assets/js/*.js",
    "analyze:size": "npm run build && du -sh build/assets/js/*.js | sort -hr",
    "analyze:tree-shaking": "npm run build && echo 'Tree shaking analysis:' && find build/assets/js -name '*.js' -exec wc -c {} + | sort -n",
    "analyze:dead-code": "node scripts/analyze-dead-code.mjs",
    "analyze:deps": "npx depcheck --ignores='@types/*,eslint*,prettier*,stylelint*,vitest*,@testing-library/*,jsdom,husky,lint-staged'",
    "analyze:exports": "node scripts/check-unused-exports.mjs",
    "analyze:all": "npm run analyze:size && npm run analyze:tree-shaking && npm run analyze:exports && npm run analyze:deps",
    "test": "vitest run --config config/vitest.config.mjs",
    "test:watch": "vitest --config config/vitest.config.mjs",
    "test:coverage": "vitest run --coverage --config config/vitest.config.mjs",
    "test:ui": "vitest --ui --config config/vitest.config.mjs",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --config config/eslint.config.js \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:css": "stylelint --config config/.stylelintrc.json \"src/**/*.css\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,scss,md,json}\"",
    "type-check": "tsc --noEmit",
    "compress:images": "node scripts/build/compress-images.mjs",
    "convert:gif:webm": "ffmpeg -y -i public/images/cat.gif -vf \"fps=24,scale=320:-1:flags=lanczos\" -c:v libvpx-vp9 -crf 36 -b:v 0 -row-mt 1 public/images/cat.webm || echo 'Install ffmpeg to run this conversion'",
    "clean": "rm -rf build dist node_modules/.vite",
    "clean:build": "rm -rf build",
    "clean:install": "npm run clean && npm install",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix",
    "outdated": "npm outdated",
    "update:check": "npm outdated",
    "update:interactive": "npx npm-check-updates -i",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.46.1",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-private-property-in-object": "^7.25.9",
    "@eslint/js": "^9.17.0",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-react": "^5.0.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.3.0",
    "npm-check-updates": "^17.1.14",
    "postcss-preset-env": "^10.1.5",
    "prettier": "^3.4.2",
    "sharp": "^0.33.5",
    "stylelint": "^16.12.0",
    "stylelint-config-recess-order": "^6.1.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-declaration-strict-value": "^1.10.0",
    "terser": "^5.44.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vite-bundle-analyzer": "^0.18.1",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^3.2.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "webpack-dev-server": "^4.15.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.16"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/guitarbeat"
  },
  "files": [
    "build",
    "public",
    "src",
    "config",
    "scripts",
    "*.md",
    "*.json"
  ],
  "volta": {
    "node": "22.12.0",
    "npm": "10.9.0"
  }
}
