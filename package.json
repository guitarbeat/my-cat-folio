{
  "name": "meow-namester",
  "version": "1.0.1",
  "description": "A React application for managing cat names and related data",
  "type": "module",
  "main": "index.js",
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaron/meow-namester.git"
  },
  "bugs": {
    "url": "https://github.com/aaron/meow-namester/issues"
  },
  "keywords": [
    "react",
    "cats",
    "names",
    "management",
    "tournament",
    "voting",
    "supabase",
    "vite"
  ],
  "author": {
    "name": "Aaron",
    "email": "aaron@example.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite --config config/vite.config.ts",
    "start": "npm run dev",
    "build": "vite build --config config/vite.config.ts",
    "build:dev": "vite build --mode development --config config/vite.config.ts",
    "build:analyze": "vite build --mode analyze --config config/vite.config.ts",
    "preview": "vite preview --config config/vite.config.ts",
    "analyze": "npm run build && npx vite-bundle-analyzer build/assets/js/*.js",
    "analyze:dead-code": "node scripts/analyze-dead-code.mjs",
    "test": "vitest run --config config/vitest.config.mjs",
    "test:watch": "vitest --config config/vitest.config.mjs",
    "test:coverage": "vitest run --coverage --config config/vitest.config.mjs",
    "test:ui": "vitest --ui --config config/vitest.config.mjs",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --config config/eslint.config.js \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:css": "stylelint --config config/.stylelintrc.json \"src/**/*.css\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,scss,md,json}\"",
    "type-check": "tsc --noEmit",
    "compress:images": "node scripts/build/compress-images.mjs",
    "convert:gif:webm": "ffmpeg -y -i public/images/cat.gif -vf \"fps=24,scale=320:-1:flags=lanczos\" -c:v libvpx-vp9 -crf 36 -b:v 0 -row-mt 1 public/images/cat.webm || echo 'Install ffmpeg to run this conversion'",
    "clean": "rm -rf build dist node_modules/.vite",
    "clean:install": "npm run clean && npm install",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix",
    "outdated": "npm outdated",
    "update:check": "npm outdated",
    "update:interactive": "npx npm-check-updates -i",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.75.0",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/plugin-transform-private-property-in-object": "^7.21.11",
    "@eslint/js": "^9.34.0",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^5.0.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "husky": "^9.0.11",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "npm-check-updates": "^17.0.0",
    "postcss-preset-env": "^10.1.5",
    "prettier": "^3.5.3",
    "sharp": "^0.34.4",
    "stylelint": "^16.23.1",
    "stylelint-config-recess-order": "^6.1.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-declaration-strict-value": "^1.10.0",
    "terser": "^5.44.0",
    "typescript": "^5.7.0",
    "vite": "^7.1.7",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^3.2.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "webpack-dev-server": "^4.15.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.16"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/aaron"
  },
  "files": [
    "build",
    "public",
    "src",
    "config",
    "scripts",
    "*.md",
    "*.json"
  ],
  "volta": {
    "node": "22.12.0",
    "npm": "10.9.0"
  }
}